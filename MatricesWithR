
###########################################################
# Creating Matrices

# Numeric Vector with 9 elements
A <- c(1,4,5,6,4,5,5,7,9) 
 
#3 by 3 matrix, assigned by column.
B <- matrix(A,nrow=3) 

C <- matrix( c(1,6,7,0.6,0.5,0.3,1,2,1), ncol=3 , byrow =TRUE)

###########################################################

# creating an identity matrix

D <- diag(3)   # 3 by 3 Identity Matrix



